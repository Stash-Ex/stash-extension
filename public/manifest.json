{
  "manifest_version": 3,

  "name": "MetaCache Extension",
  "description": "This extension will display any caches stored on the web page.",
  "version": "1.0",

  "action": {
   "default_icon": "icon.png",
   "default_popup": "index.html"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "permissions": [
   "activeTab",
   "scripting",
   "tabs"
   ]
}